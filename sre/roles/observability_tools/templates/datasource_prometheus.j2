- name: Prometheus
  uid: prometheus
  type: prometheus
  access: proxy
  url: http://{{ prometheus_service_name }}.{{ prometheus_namespace_project_name }}.svc.cluster.local:80
  user: ""
  database: ""
  basicAuth: false
  isDefault: false
  jsonData:
    httpMethod: POST
    timeInterval: 30s
    tlsAuth: false
    tlsSkipVerify: true
- name: Alertmanager
  type: alertmanager
  uid: alertmanager
  url: http://prometheus-alertmanager.{{ prometheus_namespace_project_name }}.svc.cluster.local:9093
  access: proxy
  jsonData:
    handleGrafanaManagedAlerts: false
    implementation: prometheus
